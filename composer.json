{
    "name": "rinvex/punnet",
    "description": "Rinvex Punnet is a polymorphic Laravel package, for addressbook management. You can add addresses to any eloquent model with ease.",
    "keywords": [
        "rinvex",
        "punnet",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/punnet/issues",
        "source": "https://github.com/rinvex/punnet",
        "docs": "https://github.com/rinvex/punnet/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/punnet/contributors"
        }
    ],
    "require": {
        "php": "^8.2.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Rinvex\\Addresses\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\Addresses\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Rinvex\\Addresses\\Providers\\AddressesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
